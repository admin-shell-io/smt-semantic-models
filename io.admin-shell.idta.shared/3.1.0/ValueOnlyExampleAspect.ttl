   samm:description "Example RelationshipElement with annotations."@en ;
   samm:characteristic :valueOnly4RelationshipElementWithAnnotationsCharacteristic .
   
:valueOnly4RelationshipElementWithAnnotationsCharacteristic a samm:Characteristic ;
   samm:dataType :ValueOnly4RelationshipElementWithAnnotations .
   
:ValueOnly4RelationshipElementWithAnnotations a samm:Entity ;
   samm:extends :RelationshipElementEntity ;
   samm:properties ( 
     [ samm:property :exampleAnnotations; samm:payloadName "annotations" ]
   ).

:exampleAnnotations a samm:Property ;
  samm:extends shared:annotations ;
