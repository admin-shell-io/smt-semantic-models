:TestAspect a samm:Aspect ;
   samm:preferredName "Test-Aspect"@de ;
   samm:preferredName "test Aspect"@en ;
   samm:description "Test-Aspect."@de ;
   samm:description "Test Aspect."@en ;
   samm:properties ( 
   :example4Markings 
   :languages 
   :example4specificAssetId
   :example4documentIdentifierSet 
   :externalSubjectId
   ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:example4Markings a samm:Property ;
   samm:characteristic :Markings  .
   
:example4documentIdentifierSet a samm:Property ;
   samm:characteristic :DocumentIdentifierSet  .
   
:example4specificAssetId a samm:Property ;
   samm:characteristic [ 
     a samm-c:SingleEntity ;
     samm:dataType :SpecificAssetId] .